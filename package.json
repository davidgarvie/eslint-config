{
  "name": "@davidgarvie/eslint-config",
  "version": "0.0.0-development",
  "description": "Opnionated ESLint rules for javascript projects",
  "scripts": {
    "commit": "git cz",
    "lint": "eslint -c index.js index.js",
    "lint:fix": "eslint -c index.js index.js --fix",
    "semantic-release": "semantic-release"
  },
  "main": "index.js",
  "files": ["index.js"],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davidgarvie/eslint-config.git"
  },
  "keywords": [
    "eslint"
  ],
  "author": "David Garvie",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/davidgarvie/eslint-config/issues"
  },
  "homepage": "https://github.com/davidgarvie/eslint-config#readme",
  "dependencies": {},
  "peerDependencies": {
    "eslint": ">= 6",
    "eslint-config-prettier": ">= 6",
    "eslint-plugin-import": ">= 2",
    "eslint-plugin-prettier": ">= 3",
    "prettier": ">= 2"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "semantic-release": "^17.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
